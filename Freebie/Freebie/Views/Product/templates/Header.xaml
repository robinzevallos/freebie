    xmlns:components="clr-namespace:Freebie.Components"
    xmlns:icons="clr-namespace:Freebie.Icons"
    x:Class="Freebie.Views.Product.Header"
    
    JustifyContent="SpaceBetween"
    Padding="20, 10"
